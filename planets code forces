from collections import Counter
repetition = int(input())
for _ in range(repetition):
    output = 0
    list1 = input().split()
    list2 = input().split()
    ct = Counter(list2)
    for item in ct:
        if int(ct[item])<=int(list1[1]):
            output+=int(ct[item])
        else:
            output += int(list1[1])
    print(output)
    
    
        
 
